    %body
        #main-content.wrapper

        %script{:id => "main-layout", :type => "text/template"}
            .nav-content
            .content.container
        %script{:id => "home-page", :type => "text/template"}
            .format-container
            %h3 Schedule
            .schedule-container
        %script{:id => "challenges-page", :type => "text/template"}
            .rules-container
            %h3 Challenges
            .challenges-container
        %script{:id => "navbar", :type => "text/template"}
            %nav.navbar.navbar-default{:role => "navigation"}
                .navbar-header
                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-ex1-collapse">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    </button>
                %a.navbar-brand{:href => "#"}
                    %h5 HackerOlympics.js
                .collapse.navbar-collapse.navbar-ex1-collapse
                    %ul.nav.navbar-nav
                        %li.active
                            %a{:href => "/"} Home
                        %li
                            %a{:href => "schedule/"} About
                        %li
                            %a{:href => "#contact"} Contact
                    %p.navbar-text.pull-right
                        at CascadiaJS
        %script{:id => "schedule-item", :type => "text/template"}
            .row
                .col-xs-1.col-md-2
                    %p <%= scheduletimes %>
                .col-xs-6.col-md-5
                    %p <%= scheduleevent %>
        %script{:id => "format-info", :type => "text/template"}
            %h3 Welcome
            %p <%= what %>
            %h3 Format
            %p <%= how %>
        %script{:id => "sub-view", :type => "text/template"}
            .content
        %script{:id => "challenge-item", :type => "text/template"}
            .row
                .col-xs-6.col-md-2
                    %p <%= challengetitle %>
                .col-xs-6.col-md-8
                    %p <%= rules %>
        %script{:id => "rules-item", :type => "text/template"}
            .row
                .col-xs-1.right
                    <% if (typeof(rulenumber) != "undefined") { %>
                    %p <%= rulenumber %>
                    <% } %>
                .col-xs-11
                    <% if (typeof(rulenumber) != "undefined") { %>
                    %p <%= rule %>
                    <% } else { %>
                    %h3 <%= rule %>
                    <% } %>

